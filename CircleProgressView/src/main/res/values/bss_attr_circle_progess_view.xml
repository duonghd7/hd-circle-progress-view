<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="cpvStartAngle"      format="float"/>
        <attr name="cpvSweepAngle"      format="float"/>
        <attr name="cpvStrokeWidth"     format="dimension"/>
        <attr name="cpvMode"            format="enum">
            <enum name="cpvFill"        value="0"/>
            <enum name="cpvStrokeButt"  value="1"/>
            <enum name="cpvStrokeRound" value="2"/>
        </attr>
        <attr name="cpvBackground"      format="string"/>
        <attr name="cpvProgressColor"   format="string"/>
    </declare-styleable>
</resources>